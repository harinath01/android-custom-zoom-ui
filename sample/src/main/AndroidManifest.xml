<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="us.zoom.sdksample"
      android:versionCode="1"
      android:versionName="1.0"
      android:installLocation="auto">

    <uses-feature android:glEsVersion="0x00020000"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application android:label="@string/this_app_name"
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher">

        <activity android:name="us.zoom.sdksample.ui.InitAuthSDKActivity"
            android:label="@string/this_app_name"
            android:configChanges="screenSize|orientation|keyboardHidden|keyboard"
            android:screenOrientation="portrait"
            android:icon="@drawable/ic_launcher"
            android:exported="true"
            android:theme="@style/ZMTheme.MainWindow" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".ui.LoginUserStartJoinMeetingActivity"
            android:label="@string/this_app_name"
            android:icon="@drawable/ic_launcher"

            android:exported="false"
            android:theme="@style/ZMTheme.MainWindow" >
        </activity>

        
        <activity android:name="us.zoom.sdksample.otherfeatures.MyInviteActivity"
                  android:label="@string/invite_acitivity_name"
                  android:icon="@drawable/ic_launcher"
                  android:exported="false"
                  android:theme="@style/ZMTheme.MainWindow" >
            <intent-filter>
                <action android:name="us.zoom.sdkexample.intent.action.MeetingInvite" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="us.zoom.sdksample.otherfeatures.MyWaitJoinActivity"
                  android:icon="@drawable/ic_launcher"
                  android:exported="false"
                  android:theme="@style/ZMTheme.MainWindow" >
<!--            <intent-filter>
                <action android:name="us.zoom.sdkexample.intent.action.JoinBeforeHost" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>-->
        </activity>
                
        <activity android:name="us.zoom.sdksample.otherfeatures.MyCallMeByPhoneActivity"
                  android:icon="@drawable/ic_launcher"
                  android:theme="@style/ZMTheme.MainWindow" >
<!--            <intent-filter>
                <action android:name="us.zoom.sdkexample.intent.action.CallMyPhone" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>-->
        </activity>

        <activity
            android:name="us.zoom.sdksample.inmeetingfunction.customizedmeetingui.MyMeetingActivity"
            android:configChanges="screenSize"
            android:theme="@style/ZMTheme.SubWindow"
            android:exported="false"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.MeetingSettingActivity"
            android:configChanges="screenSize"
            android:theme="@style/ZMTheme.SubWindow"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".inmeetingfunction.customizedmeetingui.RawDataMeetingActivity"
            android:configChanges="screenSize|orientation|keyboardHidden|keyboard"
            android:theme="@style/ZMTheme.SubWindow"
            android:exported="false"
            android:screenOrientation="portrait"/>
        <activity android:name=".ui.QAActivity" android:exported="false"/>
        <activity android:name=".ui.BreakoutRoomsAdminActivity"
            android:exported="false"
            android:configChanges="screenSize"
            android:theme="@style/ZMTheme.SubWindow"
            android:screenOrientation="portrait" />
        <activity android:name=".ui.BoEditActivity"
            android:exported="false"
            android:configChanges="screenSize"
            android:theme="@style/ZMTheme.SubWindow"
            android:screenOrientation="portrait" />
        <activity
            android:name="us.zoom.sdksample.inmeetingfunction.zoommeetingui.CustomZoomUIActivity"
            android:exported="false"
            android:configChanges="orientation|screenSize"
            android:hardwareAccelerated="true"
            android:theme="@style/ZMTheme.SubWindow"/>
    </application>
    
</manifest> 
